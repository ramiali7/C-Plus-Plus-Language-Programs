#include <iostream>
 
using namespace std;
 
int main() {
    float num1, num2, sum;
     
    // Taking input from user
    cout << "Enter Two Numbers\n";
    cin >> num1 >> num2;
     
    // Adding two input numbers
    sum = num1 + num2;
     
    // Printing output
    cout << "Sum of "<< num1 << " and " << num2 << " is " << sum;
    return 0;
